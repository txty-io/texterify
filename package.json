    "babel-preset-react": "^6.24.1",
    "core-js": "3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "flag-icon-css": "^3.0.0",
    "history": "^4.7.2",
    "js-file-download": "^0.4.8",
    "less-loader": "^6.0.0",
    "localforage": "^1.7.1",
    "lodash": "^4.17.15",
    "mobx": "^5.13.0",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.1.3",
    "moment": "^2.22.2",
